                                <tr>
                                    <td class="col-form-label">@item.DistrictName</td>
                                    <td class="col-form-label">@item.BlockName</td>
                                    <td class="col-form-label">@item.GPName</td>
                                    <td class="col-form-label">@item.GPCode</td>
                                    @if (@item.MC_1 == 1)
                                    {
                                        <td class="col-form-label bg-success text-light">@item.MC_1</td>
                                    }
                                    else
                                    {
