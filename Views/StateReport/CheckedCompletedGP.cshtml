@model IEnumerable<APATools.Models.Sp_APA_Checked_Complete_GPResult>
@{
    ViewBag.Title = "Report of Total District - GP wise Count";
}
<div class="page-title">
    <div class="title_left">
        <h3>Welcome to the Report of Total GP already Checked</h3>
    </div>

    <div class="title_right">
        <div class="col-md-5 col-sm-5   form-group pull-right top_search">
            <div class="input-group">
            </div>
        </div>
    </div>
</div>
<div class="clearfix"></div>
<div class="row">
    <div class="col-md-12 col-sm-12 ">
        <div class="x_panel">
            <div class="x_title">

                <h2>
                    Home of @if (Context.Session.GetString("UserName") != null)
                    {
                        <small>@Context.Session.GetString("UserName")</small>
                    }
                </h2>
                <div class="clearfix"></div>
            </div>
            @if (TempData["Failed"] != null)
            {
                <div class="x_content">
                    <div class="alert alert-danger">
                        @TempData["Failed"]
                    </div>
                </div>
            }
            @if (TempData["Success"] != null)
            {
                <div class="x_content">
                    <div class="alert alert-success">
                        @TempData["Success"]
                    </div>
                </div>
            }
            <div class="x_content">
                <div class="row">
                    <table class="table table-bordered table-striped" width="100%" id="reportid">
                        <thead>
                            <tr>
                                <th class="col-form-label">District Name</th>
                                <th class="col-form-label">Block Name</th>
                                <th class="col-form-label">GP Name</th>
                                <th class="col-form-label">GP Code</th>
                                <th class="col-form-label">MC-1</th>
                                <th class="col-form-label">MC-2</th>
                                <th class="col-form-label">MC-5</th>
                                <th class="col-form-label">MC-6</th>

                                <th class="col-form-label">TE-1</th>
                                <th class="col-form-label">TE-2</th>
                                <th class="col-form-label">TE-3</th>
                                <th class="col-form-label">TE-4</th>
                                <th class="col-form-label">TE-5</th>
                                <th class="col-form-label">TE-6</th>
                                <th class="col-form-label">TE-7</th>
                                <th class="col-form-label">TE-8</th>
                                <th class="col-form-label">TE-9</th>
                                <th class="col-form-label">TE-10</th>
                                <th class="col-form-label">TE-11</th>
                                <th class="col-form-label">TE-13</th>
                                <th class="col-form-label">TE-14</th>
                                <th class="col-form-label">TE-16</th>
                                <th class="col-form-label">TE-17</th>
                                <th class="col-form-label">TE-19</th>
                                <th class="col-form-label">TE-20</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
